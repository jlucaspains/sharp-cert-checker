		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.1",
		"postcss": "^8.4.49",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.2",
		"svelte": "^5.14.0",
		"svelte-check": "^4.1.1",
		"tailwindcss": "^3.4.16",
