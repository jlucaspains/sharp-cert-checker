		"eslint-plugin-svelte": "^2.43.0",
		"postcss": "^8.4.41",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"svelte": "^4.2.19",
		"svelte-check": "^3.8.6",
		"tailwindcss": "^3.4.10",
