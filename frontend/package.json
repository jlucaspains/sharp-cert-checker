		"@sveltejs/kit": "^2.5.5",
		"autoprefixer": "^10.4.19",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"postcss": "^8.4.38",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.2",
		"svelte": "^4.2.12",
		"svelte-check": "^3.6.9",
		"tailwindcss": "^3.4.3",
		"typescript": "^5.4.4",
		"vite": "^5.2.8"
